{% load static %}
doctype 5
html(lang="en")
  head
    title Videodrome
    link(href='{% get_static_prefix %}css/custom.css?body=1',media='all',rel='stylesheet',type='text/css')
    script(src='{% get_static_prefix %}js/jquery-1.7.1.min.js')
    
  body
    div.app-container
      div.row.register-login
        div.columns.six.register
          h3 Register!
        div.columns.six.log-in
          h3 Log In!
      div.placed-fixed
        div.row.media-player
          div.media-player.four.offset-by-eight
            div.youtube-placement
        div.media-control
          a.prev-media(href='#') Prev
          a.next-media(href='#') Next
      div.row
        div.columns.three.recent-container
          div.logo THIS IS VIDEODROME
          div.recent-favorites
            h4 Your Recent Favorites
            ul
              a.track(data-id='1337',href='#')
